<settings>
    <profiles>
        <profile>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>central</id>
                    <url>https://central.maven.org/maven2/</url>
                </repository>
                <repository>
                    <id>mirror</id>
                    <url>https://repo1.maven.org/maven2/</url>
                </repository>
                <repository>
                    <id>mycompany-maven</id>
                    <url>https://${env.ARTIFACT_STORE_HOST}/maven/maven/</url>
                </repository>
            </repositories>
            <!-- this is for plugins' dependencies -->
            <pluginRepositories>
                <pluginRepository>
                    <id>central</id>
                    <url>https://central.maven.org/maven2/</url>
                </pluginRepository>
                <pluginRepository>
                    <id>mirror</id>
                    <url>https://repo1.maven.org/maven2/</url>
                </pluginRepository>
                <pluginRepository>
                    <id>mycompany-maven</id>
                    <url>https://${env.ARTIFACT_STORE_HOST}/maven/maven/</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
    <servers>
        <server>
            <id>mycompany-maven</id>
            <username>aws</username>
            <password>${env.ARTIFACT_STORE_TOKEN}</password>
        </server>
    </servers>
</settings>
